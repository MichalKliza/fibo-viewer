		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/Loans/LoansGuaranties/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/Loans/LoansParties/"/>
