	<owl:Class rdf:about="&fibo-loan-typ-mtg;MortgageLoan">
		<rdfs:subClassOf rdf:resource="&fibo-loan-typ-sec;CollateralizedSecuredLoan"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-loan-typ-mtg;hasBorrowingParty"/>
