		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoanContracts/LoanCore/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoanTypes/SecuredLoans/"/>
