<?xml version="1.0" encoding="UTF-8"?>
<properties>
  
  <!-- To load an ontology from the internet, enter the URL between the <ontologyURL> tags -->
  <!-- To load an ontology from a folder, enter the path address with application between <ontologyPath> tags or enter the file name  -->
  <!-- If no URL or path is provided, the program will load the ontology from the default folder -->
  <ontology>
    <ontologyURL>https://spec.edmcouncil.org/fibo/ontology/master/2019Q3/AboutFIBOProd.ttl</ontologyURL>
    <!--<ontologyPath>C:\Users\user\.weasel\AboutFIBOProd.rdf</ontologyPath>-->
    <!--<ontologyPath>AboutFIBOProd.rdf</ontologyPath>--> 
  </ontology>
  
  <ignoreToDisplaying>
    <ignore>http://viewer.axiom#SubObjectPropertyOf</ignore>
  </ignoreToDisplaying>
       
  <!-- This is configuration about labels and displaying values -->
  <labelConfig>
      
    <!-- When TRUE - (default) labels will be extracted and displayed, FALSE - displayed IRI fragment's or defined user labels -->
    <displayedLabels>TRUE</displayedLabels>
      
    <!-- When DEFINED (default) -  rename configuration is more important than labels at group element names, -->
    <!-- EXTRACTED - labels is more important than other, if present label be put -->
    <labelPriority>DEFINED</labelPriority>
      
    <!-- When TRUE - only labels with given lang be displayed, if language is not present displayed is fragment, -->
    <!-- FALSE - (default) does not force labels to have a language -->
    <forceLabelLang>FALSE</forceLabelLang>
      
    <!-- When an item has more than one label, the one with the given language will be displayed (default is en) -->
    <labelLang>en</labelLang>
      
    <!-- Define action when label is not forced and entity has more than one label but noone have a language  -->
    <!-- FRAGMENT - assigns an IRI fragment to the label -->
    <!-- FIRST - (default) assigns an first element of label list -->
    <missingLanguageAction>FIRST</missingLanguageAction>
    
    <!-- Here we can define some default labels for specific URI -->
    <defaultLabelList>
      
      <defaultLabelDef>
        <defaultLabelIri>http://www.w3.org/2000/01/rdf-schema#Literal</defaultLabelIri>
        <defaultLabelValue>literal</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://www.w3.org/2001/XMLSchema#string</defaultLabelIri>
        <defaultLabelValue>string</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://www.omg.org/techprocess/ab/SpecificationMetadata/fileAbbreviation</defaultLabelIri>
        <defaultLabelValue>file abbreviation</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://www.omg.org/techprocess/ab/SpecificationMetadata/filename</defaultLabelIri>
        <defaultLabelValue>file name</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://www.w3.org/2000/01/rdf-schema#isDefinedBy</defaultLabelIri>
        <defaultLabelValue>is defined by</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#EquivalentClasses</defaultLabelIri>
        <defaultLabelValue>Equivalent classes (necessary and sufficient criteria)</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#SubClassOf</defaultLabelIri>
        <defaultLabelValue>IS-A restrictions (necessary criteria)</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#DisjointClasses</defaultLabelIri>
        <defaultLabelValue>Disjoint classes</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#ClassAssertion</defaultLabelIri>
        <defaultLabelValue>Instance classification</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#SameIndividual</defaultLabelIri>
        <defaultLabelValue>same as</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#ObjectPropertyAssertion</defaultLabelIri>
        <defaultLabelValue>Object property assertion</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#DataPropertyAssertion</defaultLabelIri>
        <defaultLabelValue>Data property assertion</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#SubObjectPropertyOf</defaultLabelIri>
        <defaultLabelValue>SubObject property</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#SubDataPropertyOf</defaultLabelIri>
        <defaultLabelValue>SubData property</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#SubAnnotationPropertyOf</defaultLabelIri>
        <defaultLabelValue>SubAnnotation property</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#EquivalentObjectProperties</defaultLabelIri>
        <defaultLabelValue>Equivalent object properties</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#ObjectPropertyDomain</defaultLabelIri>
        <defaultLabelValue>Property domain</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#ObjectPropertyRange</defaultLabelIri>
        <defaultLabelValue>Property range</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://viewer.axiom#InverseObjectProperties</defaultLabelIri>
        <defaultLabelValue>Inverse object properties</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/synonym</defaultLabelIri>
        <defaultLabelValue>synonym(s)</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://www.w3.org/2000/01/rdf-schema#seeAlso</defaultLabelIri>
        <defaultLabelValue>see also</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://purl.org/dc/terms/hasPart</defaultLabelIri>
        <defaultLabelValue>has part</defaultLabelValue>
      </defaultLabelDef>
      
      <defaultLabelDef>
        <defaultLabelIri>http://purl.org/dc/terms/contributor</defaultLabelIri>
        <defaultLabelValue>contributor</defaultLabelValue>
      </defaultLabelDef>
      
  
    </defaultLabelList>
    
  </labelConfig>
  
  <!--Feature will be added if future
  <ignoreToLinking>
    <ignore>http://www.some.iri#ignoreToLinking</ignore>
  </ignoreToLinking>-->

  <!-- Here we define URIs to be treated as IRIs -->
  <!-- We are currently using the method contains(), please be careful when define URIs -->
  <scopeIri>
    <uriNamespace>https://spec.edmcouncil.org/fibo/ontology</uriNamespace>
    <uriNamespace>https://www.omg.org/spec/</uriNamespace>
    <uriNamespace>http://www.omg.org/techprocess</uriNamespace>
  </scopeIri>
    

  <priorityList>
    <!--This list will be used when groups is not present-->
    <priority>http://www.w3.org/2000/01/rdf-schema#subClassOf</priority>
    <priority>http://www.w3.org/2000/01/rdf-schema#seeAlso</priority>
  </priorityList>
   
  <groups>
    <!--The order of elements in groups will be their priority-->
    <group>
      <groupName>Natural Language characteristic</groupName>
      <groupItem>http://www.w3.org/2000/01/rdf-schema#label</groupItem>
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/synonym</groupItem>
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/abbreviation</groupItem>
      <groupItem>http://www.w3.org/2004/02/skos/core#definition</groupItem>
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/definitionOrigin</groupItem>
      <groupItem>http://purl.org/dc/terms/source</groupItem>         
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/adaptedFrom</groupItem>
      <groupItem>http://www.w3.org/2004/02/skos/core#example</groupItem>
      <groupItem>http://www.w3.org/2004/02/skos/core#scopeNote</groupItem>
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/explanatoryNote</groupItem>
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/usageNote</groupItem>
      <groupItem>http://www.w3.org/2004/02/skos/core#editorialNote</groupItem>
      <groupItem>http://purl.org/dc/terms/title</groupItem>
      <groupItem>http://purl.org/dc/terms/abstract</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/keyword</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/fileAbstract</groupItem> 
    </group>

    <group>
      <groupName>Ontological characteristic</groupName>
      <groupItem>http://viewer.axiom#EquivalentClasses</groupItem>
      <groupItem>http://viewer.axiom#SubClassOf</groupItem>
      <groupItem>http://viewer.function#anonymous_ancestor</groupItem>
      <groupItem>http://viewer.axiom#DisjointClasses</groupItem>
      <groupItem>http://viewer.function#direct_subclasses</groupItem>
      <groupItem>http://viewer.function#instances</groupItem>
      <groupItem>http://viewer.axiom#ClassAssertion</groupItem>
      <groupItem>http://viewer.axiom#SameIndividual</groupItem>
      <groupItem>http://viewer.axiom#ObjectPropertyAssertion</groupItem>
      <groupItem>http://viewer.axiom#DataPropertyAssertion</groupItem>
      <groupItem>http://viewer.axiom#SubObjectPropertyOf</groupItem>
      <groupItem>http://viewer.axiom#SubDataPropertyOf</groupItem>
      <groupItem>http://viewer.axiom#SubAnnotationPropertyOf</groupItem>
      <groupItem>http://viewer.axiom#EquivalentObjectProperties</groupItem>
      <groupItem>http://viewer.axiom#ObjectPropertyDomain</groupItem>
      <groupItem>http://viewer.axiom#ObjectPropertyRange</groupItem>
      <groupItem>http://viewer.axiom#InverseObjectProperties</groupItem>
      <groupItem>http://purl.org/dc/terms/hasPart</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/dependsOn</groupItem>
    </group>

    <group>
      <groupName>Meta-information</groupName>
      <groupItem>https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/hasMaturityLevel</groupItem>
      <groupItem>http://purl.org/dc/terms/creator</groupItem>
      <groupItem>http://purl.org/dc/terms/license</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/copyright</groupItem>
      <groupItem>http://purl.org/dc/terms/contributor</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/contributor</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/moduleAbbreviation</groupItem>         
      <groupItem>http://purl.org/dc/terms/issued</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/contentLanguage</groupItem>
      <groupItem>http://www.w3.org/2004/02/skos/core#changeNote</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/fileAbbreviation</groupItem>
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/filename</groupItem>         
      <groupItem>http://www.w3.org/2000/01/rdf-schema#comment</groupItem>
      <groupItem>http://www.w3.org/2000/01/rdf-schema#isDefinedBy</groupItem>
      <groupItem>http://www.w3.org/2000/01/rdf-schema#seeAlso</groupItem>
      <groupItem>http://www.w3.org/2004/02/skos/core#historyNote</groupItem> 
      <groupItem>http://www.omg.org/techprocess/ab/SpecificationMetadata/relatedSpecification</groupItem>                  
    </group>
    
    <group>
      <groupName>Resources</groupName>
      
      <groupItem>http://viewer.external#clazz</groupItem>
      <groupItem>http://viewer.internal#clazz</groupItem>
		
      <groupItem>http://viewer.external#dataProperty</groupItem>
      <groupItem>http://viewer.internal#dataProperty</groupItem>
		
      <groupItem>http://viewer.external#instance</groupItem>
      <groupItem>http://viewer.internal#instance</groupItem>
		
      <groupItem>http://viewer.external#objectProperty</groupItem>
      <groupItem>http://viewer.internal#objectProperty</groupItem>
    </group>
  </groups>
    
</properties>